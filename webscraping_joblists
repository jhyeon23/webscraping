      "cell_type": "code",
      "source": [
        "apply = []\n",
        "for i in range(0,10):\n",
        "  url = full_url[i]\n",
        "  response = requests.get(url)\n",
        "  soup = bs(response.content, 'html.parser')\n",
        "\n",
        "  apply_link = soup.find('a', string=\"Apply\")\n",
        "  if apply_link and apply_link.has_attr('href'):\n",
        "    apply_url = apply_link['href']\n",
        "    apply.append(apply_url)\n",
        "  else:\n",
        "    apply.append('NA')"
      ],
      "metadata": {
        "id": "LM3jpPI9S2ph"
