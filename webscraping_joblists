        "\n",
        "for i in range(0,10):\n",
        "    url = full_url[i]\n",
        "    response = requests.get(url)\n",
        "    soup = bs(response.content, 'html.parser')\n",
        "\n",
        "    position_id_tag = soup.find('b', string=\"Position ID:\")\n",
        "\n",
        "    if position_id_tag:\n",
        "        next_div = position_id_tag.parent.find_next_sibling('div')\n",
        "        if next_div:\n",
